{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",

  "description": "Manually Made Chart",

   "title": {
    "text": "Manually Made Chart",
    "subtitle":"Chart made, with the data being manually written in",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },

  "data": {
    "values": [
      {"x": "UK", "y": 70}, {"x": "France", "y": 32}, {"x": "Germany", "y": 48},
      {"x": "Poland", "y": 56}, {"x": "Romania", "y": 92},
    ]
  },
  "height": 300,
  "width": 345,
  "mark": {
    "type": "line",
    "point": false,
    "interpolate": "monotone",
    "color": "blueviolet"
  },
  "encoding": {
    "x": {
      "field": "x",
      "type": "nominal",
      "axis": {
        "title": null,
        "grid": false,
        "labelSeparation": 20,
        "ticks": false
      }
    },
    "y": {
      "field": "y",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    }
  }
}

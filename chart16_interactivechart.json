{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Interactive Chart",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/EliezerMeyer/EliezerMeyer.github.io/main/Annual%20Growth%20Rate%20of%20Atmospheric%20Carbon%20Dioxide.csv",
    "format": {"property": "csv"}
  },
  "height": 500,
  "width": 710,
  "mark": {"type": "line"},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false},
      "sort": "ascending"
    },
    "y": {
      "field": "Annual Increase",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": false}
    }
  }
}
